{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "frost-child/alphabet-filter",
  "version": "1.0.0",
  "title": "A–Ö filter",
  "category": "widgets",
  "icon": "search",
  "description": "Sök + A–Ö-filtrering för inlägg.",
  "textdomain": "frost-child",
  "editorScript": "file:./edit.js",
  "script": "file:./view.js",
  "style": "file:./style.css",
  "editorStyle": "file:./editor.css",
  "attributes": {
    "postType": {
      "type": "string",
      "default": "post"
    },
    "perPage": {
      "type": "number",
      "default": 100
    },
    "placeholder": {
      "type": "string",
      "default": "Sök efter ..."
    },
    "allLabel": {
      "type": "string",
      "default": "ALLA"
    }
  },
  "supports": {
    "html": false,
    "align": false
  }
}
